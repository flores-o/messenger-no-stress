{
    "manifest_version": 2, 
    "name": "Messenger focus-mode",
    "description": "Eliminate distractions by hiding the message list. You can still search for a particluar conversation and chat.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.messenger.com/*",
                "http://www.messenger.com/*"
            ],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png"
    }

}
